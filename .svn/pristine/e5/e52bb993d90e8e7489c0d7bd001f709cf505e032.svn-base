.table { display:table; }
.table-row { display:table-row; }
.table-cell { display:table-cell; }

#campaign-header{
  font-weight:bold;
  text-align:center;
  margin-top: 4%;
}

#nothing-header{
  text-align:center;
  margin-top: 4%;
}

#landing-page-header{
  text-align:center;
  font-weight:bold;
}

.container-header{
  text-align: center;
  margin-bottom: 20px;
}

#cv-container{
  width:70%;
  background-color: #CCCCCC;
  border-radius: 10px;
  box-shadow: 5px 5px 10px rgba(0,0,0,0.5);
  margin-top: 5%;
}

#survey-container{
  width:80%;
  background-color: #CCCCCC;
  border-radius: 10px;
  box-shadow: 5px 5px 10px rgba(0,0,0,0.5);
  margin-top: 5%;
}

.cv-header{
  font-weight:bold;
  text-align:center;
}

.cv-select{
  text-align:center;
  select{
    font-size:1.2em;
    width:100%;
    padding: 5px 0 5px 0;
  }
  a{
    display:block;
    width: 75%;
    margin: 5px auto 10px auto;
    border-width: 0 3px 3px 0;
    border-radius: 3px;
    border-color: #BBBBBB;
    border-style:solid;
    background-color: #EEEEEE;
    padding: 5px 0 5px 0;
  }
}

#campaign-form{
  width: 70%;
  background-color: #CCCCCC;
  border-radius: 10px;
  box-shadow: 5px 5px 10px rgba(0,0,0,0.5);
  margin-top: 2%;
  padding: 20px;
  .field{
    float:left;
    text-align:center;
  }
}

.back{
      a{
        background-color: #EEEEEE;
        padding:10px;
        margin-top: 20px;
        margin-bottom: 10px;
        border-radius: 3px;
        border-width:0 3px 3px 0;
        border-style:solid;
        border-color: #AAAAAA;
        font-size:12px;
        margin-left:25%;
        display:block;
        max-width: 150px;
        text-align:center;
      }
}

#campaign-name{
  width: 100%;
  input{
    width:100%;
  }
  label{
  }
}

.cv-form-label{
  text-align:center;
}

.cv-form-input{
  
}

.modal-body{
  width:90%;
  margin-left:auto;
  margin-right:auto;
  background-color: white;
  border-style: solid;
  border-color: gray;
  border-width: 2px 2px 2px 2px;
  border-radius: 20px;
  box-shadow: 5px 5px 10px rgba(0, 0, 0, 0.5);
  #form-container{
    margin: 0 auto 0 auto;
    width:100%;
    border-style:none;
    box-shadow:none;
  }
  #inside-manager-close{
    text-align:right;
  }
}

#vendor-specialist-table{
  width:50%;
  float:left;
  position:relative;
  z-index:10;
}

#specialist-vendor-rows{
  width:50%;
  float:right;
  position:relative;
  z-index:10;
}

#manager-table{
  margin-left:auto;
  margin-right:auto;
  margin-top: 3%;
  border-radius: 3px;
}

.manager-table-header{
  border-style:solid;
  border-width: 1px;
  border-color:black;
  padding:10px;
  font-weight:bold;
  background-color:#BBBBBB;
  color:black;
}

.manager-table-data{
  border-style:solid;
  border-width: 1px;
  border-color:black;
  padding:10px;
  text-align:center;
}

.edit-btn{
  border-radius: 5px;
  background-color: #CCCCCC;
  border-width: 0 3px 3px 0;
  border-style: solid;
  border-color: #AAAAAA;
  padding: 5px;
  display:inline-block;
  margin-left:10px;
  margin-bottom: 10px;
  text-align:center;
}

.destroy-btn{
  border-radius: 5px;
  background-color: #FD7777;
  border-width: 0 3px 3px 0;
  border-style: solid;
  border-color: #EA9C9C;
  padding: 5px;
  display:inline-block;
  margin-left:10px;
  margin-bottom: 10px;
  text-align:center;
}

.new-btn{
  border-radius: 10px;
  background-color: #CCCCCC;
  border-width: 0 3px 3px 0;
  border-style: solid;
  border-color: #AAAAAA;
  padding: 10px;
  display:inline-block;
  margin-left: 20%;
  margin-top: 3%;
}

#question-container{
  width: 100%;
  background-color: #CCCCCC;
  box-shadow: 5px 5px 10px rgba(0, 0, 0, 0.5);
  margin-top: 2%;
  padding:0;
}

#add_field{
 padding:5px; 
 background-color: #DDDDDD;
 border-radius: 5px;
 border-style: solid;
 border-width: 0 2px 2px 0;
 border-color: #AAAAAA;
 transition: all 200ms;
 display:inline-block;
 margin-left:20%;
 margin-bottom:10px;
}

#add_field:hover{
  color: black;
  background-color: #999999;
}

.remove_fields{

}
.remove_fields:hover{
  opacity:1.0;
  background-color: transparent;
}

  .actions{
    text-align:center;
    input, a{
      background-color: #EEEEEE;
      padding:10px;
      margin-top: 20px;
      border-radius: 3px;
      border-width:0 3px 3px 0;
      border-style:solid;
      border-color: #AAAAAA;
      transition: all 200ms;
    }
    input{
      font-weight:bold;
    }
    input:hover{
      color: black;
      background-color: #999999;
    }
  }
  
  .question-field{
    text-align:center;
    background-color: #DDDDDD;
    border-radius: 10px;
    width:100%;
    padding-bottom: 5px;
    float:left;
    font-size:10px;
    padding:10px;
    p{
      margin-top: 0px;
      margin-bottom:0px;
      float:left;
      font-size:10px;
    }
    select{
      margin-right: 4px;
      float:left;
      font-size:10px;
    }
    input{
      font-size:10px;
      line-height:normal;
    }
  }
  
  #add-reviewers{
    margin-left: auto;
    margin-right:auto;
    tbody{
      tr{
        padding:5px;
        td{
          padding:5px;
        }
      }
    }
  }
  
  #reviewer-header{
    font-size:1.2em;
    font-weight:bold;
    text-align:center;
    margin-top: 8px;
  }
  
  .reviewer-field{
    width: 100%;
    text-align:center;
    background-color: #DDDDDD;
    border-radius: 10px;
    padding: 20px;
    float:left;
  }
  
  .iterator{
  font-size: 2em;
  padding:5px;
  text-align:left;
  font-weight:bold;
  float:left; 
}

.weight-field{
  margin-top:0px;
  margin-bottom:0px;
  float:left;
}

.role-field{
  float:left;
  .col-md-2{
    padding:0;
  }
}

.answer-field{
  float:left;
  .answer-option{
    padding:0;
    display:inline-block;
  }
}

.answer-option{
  margin-top:5px;
  margin-bottom:5px;
}

.modal-backdrop{
  min-height:1000px;
}

.review-row{
  background-color: #FFF3D2 !important;
}

.end-date-row{
  background-color: #FAD6D6 !important;
  .sorting_1{
    background-color: #FFB6B6 !important;
  }
}

.complete-row{
  background-color: #C3FFD4 !important;
  .sorting_1{
    background-color: #B5FFC9 !important;
  }
}

.process-row{
  background-color: #CAE1FF !important;
  .sorting_1{
    background-color: #AED1FF !important;
  }
}

thead{
  font-size:10px;
}

td{
  border-width: 1px 1px 1px 1px;
  border-style: solid;
  border-color: #EDE7E7;
}

.question{
  width:100%;
  margin: 10px auto 10px auto;
  display:block;
  border-radius: 15px;
  background-color: #EEEEEE;
}

.question-row{
  margin-left:auto;
  margin-right:auto;
  th{
    text-align:center;
    padding: 5px;
  }
}

.answer-row{
  margin-left:auto;
  margin-right:auto;
  text-align:center;
}

.answer{
  width:13%;
  float:left;
  text-align:center;
  margin:5px;
  font-size:12px;
  border-style: solid;
  border-width: 1px;
  border-color: #DDDDDD;
  border-radius: 10px;
  padding: 5px;
}

.survey-radio{
  text-align:center;
  margin-left:auto;
  margin-right:auto;
}

.button-row{
  text-align:center;
}

.form-button-2{
  border-radius: 5px;
  background-color: #DDDDDD;
  border-width: 0 3px 3px 0;
  border-style: solid;
  border-color: #AAAAAA;
  padding: 10px 20px;
  display: inline-block;
  margin-left: 10px;
  margin-top: 20px;
}

.progress{
  transition: width 300ms;
}

.progress-cell{
  text-align:center;
}

.bar_container{
    height: inherit !important;
}

.container_tiny{
  padding: 3px !important;
}

.mortice_tiny{
  height: 6px !important;
}

.progress_tiny{
  border-style: none !important;
  margin-bottom: 0px !important;
}

#survey-bar{
  text-align: center;
  color: white;
  font-weight: bold;
  width: 20%;
  margin-bottom:2%;
}

#number{
  position: absolute;
  left: 30%;
  right: 30%;
}

.number{
  text-align:center;
}

td{
