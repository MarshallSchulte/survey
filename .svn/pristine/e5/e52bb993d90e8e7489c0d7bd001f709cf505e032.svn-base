    }
  }
}

.specialist-header{
  font-size:10px;
}

.vendor-header{
  font-size:10px;
}

.vendor-box{
  padding: 0 !important;
}

.vendor-row{
  text-align:center;
  border-color: #83B291;
  border-style:solid;
  border-width: 1px 1px 1px 2px;
}

.vendor-row:first-child{
  border-width: 0px 1px 1px 2px;
}

.vendor-row:last-child{
  border-width: 1px 1px 0px 2px;
}

.vendor-row:only-child{
  border-width: 0px 1px 0px 2px;
}

.specialist-assigned{
  background-color: #FAD6D6;
  border-color: #FF9191;
}

.specialist-unassigned{
