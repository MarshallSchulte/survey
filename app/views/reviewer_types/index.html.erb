<h1>Listing reviewer_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reviewer_types.each do |reviewer_type| %>
      <tr>
        <td><%= reviewer_type.name %></td>
        <td><%= link_to 'Show', reviewer_type %></td>
        <td><%= link_to 'Edit', edit_reviewer_type_path(reviewer_type) %></td>
        <td><%= link_to 'Destroy', reviewer_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reviewer type', new_reviewer_type_path %>
